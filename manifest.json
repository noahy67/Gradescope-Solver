{
    "manifest_version": 3,
    "name": "Gradescope Quiz Solver",
    "description": "Solve your Gradescope quiz with one click!",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "gradescopesolver_logo.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript.js"]
        }
    ]
  }